@{
    Layout = null;
}
@await Component.InvokeAsync("_HeadViewComponentPartial")
<body>
    @await Component.InvokeAsync("_PreloaderViewComponentPartial")
    <div id="main-wrapper">
        @await Component.InvokeAsync("_NavbarViewComponentPartial")
        @await Component.InvokeAsync("_HeaderViewComponentPartial")
        @await Component.InvokeAsync("_SidebarViewComponentPartial")
        <div class="content-body">
            <div class="row page-titles mx-0">
                <div class="col p-md-0">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="javascript:void(0)">Dashboard</a></li>
                        <li class="breadcrumb-item active"><a href="javascript:void(0)">Home</a></li>
                    </ol>
                </div>
            </div>
            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @await Component.InvokeAsync("_FooterViewComponentPartial")
        @await Component.InvokeAsync("_ScriptViewComponentPartial")
</body>
</html>